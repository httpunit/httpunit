<VirtualHost *:80>

	ServerName 127.0.0.1
	DocumentRoot %TRAVIS_BUILD_DIR%/test/application

<Directory %TRAVIS_BUILD_DIR%/test/application>
	Options FollowSymLinks MultiViews ExecCGI
	AllowOverride All
	Order deny,allow
	Allow from all
</Directory>

<LocationMatch "^(.*\.php)$">
  ProxyPass fcgi://127.0.0.1:9000%TRAVIS_BUILD_DIR%
</LocationMatch>

</VirtualHost>
